enum Color:byte { Red = 0, Blue, Green, Purple, Black, Orange, Cyan, Pink = 7}

struct Vector2 {
    x: float32;
    y: float32;
}

table Player {
    id: uint32;
    pos: Vector2;
    vel: Vector2;
    color: Color = Red;
    grounded: bool;
    jump_timer: float32;
    size: float32;
}

table GameState {
  players: [Player];
}

root_type GameState;
